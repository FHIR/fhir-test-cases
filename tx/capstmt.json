{
  "resourceType": "CapabilityStatement",
  "url": "$url$",
  "version": "$token$",
  "name": "$token$",
  "title": "$string$",
  "status": "active",
  "date": "$date$",
  "kind": "instance",
  "instantiates": [
    "http://hl7.org/fhir/CapabilityStatement/terminology-server"
  ],
  "software": {
    "name": "$string$",
    "version": "$token$",
    "releaseDate": "$date$"
  },
  "fhirVersion": "$version$",
  "format": ["application/fhir+json"],
  "rest": [{
    "mode": "server",
    "resource": [
      {
        "type": "CodeSystem",
        "operation": [{
          "name": "lookup",
          "definition": "$url$"
        },{
          "name": "validate-code",
          "definition": "$url$"
        }]
      },
      {
        "type": "ValueSet",
        "interaction": [{
          "code": "read"
        },{
          "code": "search-type"
        }],
        "operation": [
          {
            "name": "expand",
            "definition": "$url$"
          },
          {
            "name": "validate-code",
            "definition": "$url$"
          }
        ]
      }
    ],
    "operation": [{
      "name": "versions",
      "definition": "$url$"
    }]
  }]
}

